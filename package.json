  "scripts": {
    "babel-node": "babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'",
    "build": "webpack",
    "dev": "webpack-dev-server --open",
    "serve": "concurrently \"npm run start\" \"npm run dev\"",
    "start": "nodemon Server.js",
    "prettier": "npx prettier --write .",
    "restart": "npm run prettier && rm -rf package-lock.json && rm -rf node_modules && npm install && npx npm-check-updates -u && npm install && npm run serve",
    "push": "npm run prettier && rm -rf package-lock.json && rm -rf node_modules && git add . && git commit -m 'complete' && git push",
    "kill": "sudo kill -9 `ps aux | grep node | grep -v grep | awk '{print $2}'`",
    "clean": "npm cache clean --force && npm run build"
  },
